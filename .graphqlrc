schema: "https://brand-service.api.genera.space/graphql"
extensions:
  graphql-markdown:
    baseURL: "."
    homepage: "static/index.md"
    loaders:
      UrlLoader: "@graphql-tools/url-loader"
    docOptions:
      frontMatter:
        pagination_next: null
        pagination_prev: null
    printTypeOptions:
      deprecated: "group"
