schema: "http://localhost:5050/graphql"
extensions:
  graphql-markdown:
    baseURL: "."
    loaders:
      UrlLoader: "@graphql-tools/url-loader"
    diffMethod: "schema-diff"
    homePage: "static/index.md"
    docOptions:
      frontMatter:
        pagination_next: null
        pagination_prev: null
    printTypeOptions:
      deprecated: "group"
